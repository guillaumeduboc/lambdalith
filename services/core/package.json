{
  "name": "@lambdalith/core",
  "private": true,
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "bootstrap": "cdk bootstrap --profile guillaumed --qualifier de91078a5b",
    "bootstrap-production": "cdk bootstrap --context stage=production --qualifier de91078a5b",
    "bootstrap-staging": "cdk bootstrap --context stage=staging --qualifier de91078a5b",
    "deploy": "cdk deploy --profile guillaumed",
    "deploy-production": "cdk deploy --context stage=production",
    "deploy-staging": "cdk deploy --context stage=staging",
    "destroy": "cdk destroy --profile guillaumed",
    "destroy-production": "cdk destroy --context stage=production",
    "destroy-staging": "cdk destroy --context stage=staging",
    "format-check": "prettier --check . --ignore-path ../../.prettierignore",
    "format-fix": "prettier --write . --ignore-path ../../.prettierignore",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts",
    "test": "nx run format-check && nx run test-linter && nx run test-type && nx run test-unit && nx run test-circular",
    "test-circular": "pnpm depcruise --validate .dependency-cruiser.js .",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc",
    "test-unit": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.518.0",
    "@aws-sdk/lib-dynamodb": "^3.518.0",
    "@codegenie/serverless-express": "^4.13.0",
    "@fastify/aws-lambda": "^4.0.0",
    "@hono/zod-validator": "^0.2.0",
    "@lambdalith/cdk-configuration": "workspace:^",
    "@middy/core": "^5.2.3",
    "@middy/http-cors": "^5.2.3",
    "@swarmion/serverless-contracts": "^0.33.0-alpha.1",
    "@swarmion/serverless-helpers": "^0.33.0-alpha.1",
    "@trpc/server": "^10.45.0",
    "ajv": "^8.12.0",
    "body-parser": "^1.20.2",
    "dynamodb-toolbox": "1.0.0-alpha.20240129",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "fastify": "^4.26.1",
    "fastify-type-provider-zod": "^1.1.9",
    "hono": "^4.0.4",
    "morgan": "^1.10.0",
    "zod": "^3.22.4",
    "zod-express-middleware": "^1.4.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.133",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.10",
    "@vitest/coverage-v8": "^0.34.6",
    "aws-cdk": "2.114.1",
    "aws-cdk-lib": "2.114.1",
    "constructs": "10.3.0",
    "dependency-cruiser": "^16.1.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^0.34.6"
  }
}
